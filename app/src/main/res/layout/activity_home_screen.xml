<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeScreen">


    <ImageView
        android:id="@+id/IMG_background_home"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/backgroundhome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="@+id/IMG_background_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_AprendeMas2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="842dp" />

            <TextView
                android:id="@+id/TV_Aprendemas"
                android:layout_width="274dp"
                android:layout_height="50dp"
                android:fontFamily="@font/ralewaybold"
                android:gravity="start|left"
                android:padding="5dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/H_aprendeMas"
                android:textColor="@color/white"
                android:textSize="22sp"
                app:layout_constraintBottom_toTopOf="@+id/guideline_AprendeMas2"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/guideline_AprendeMas"
                app:layout_constraintVertical_bias="0.428" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_AprendeMas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="778dp" />

            <ImageView
                android:id="@+id/iv_menu_home"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="30dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/menu_icon" />

            <androidx.cardview.widget.CardView
                android:id="@+id/menu_view_home"
                style="@style/CardViewStyle_5"
                android:layout_width="338dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:elevation="50dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.511"
                    tools:layout_editor_absoluteX="-55dp">

                    <ImageView
                        android:id="@+id/iv_x_menu"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginTop="50dp"
                        android:layout_marginRight="20dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/x_icon"></ImageView>

                    <ImageView
                        android:id="@+id/iv_home_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/home_icon" />

                    <TextView
                        android:id="@+id/tv_home_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="48dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/home_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/iv_news_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv_home_icon"
                        app:srcCompat="@drawable/news_icon" />

                    <TextView
                        android:id="@+id/tv_news_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="36dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/news_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_home_icon" />

                    <ImageView
                        android:id="@+id/iv_launches_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv_news_icon"
                        app:srcCompat="@drawable/launches_icon" />

                    <TextView
                        android:id="@+id/tv_launches_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="36dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/launches_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_news_icon" />

                    <ImageView
                        android:id="@+id/iv_mars_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv_launches_icon"
                        app:srcCompat="@drawable/mars_icon" />

                    <TextView
                        android:id="@+id/tv_mars_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="36dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/mars_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_launches_icon" />

                    <ImageView
                        android:id="@+id/iv_satellites_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv_mars_icon"
                        app:srcCompat="@drawable/satellites_icon" />

                    <TextView
                        android:id="@+id/tv_satellites_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="36dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/satellites_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_mars_icon" />

                    <ImageView
                        android:id="@+id/iv_settings_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv_satellites_icon"
                        app:srcCompat="@drawable/settings_icon" />

                    <TextView
                        android:id="@+id/tv_settings_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="56dp"
                        android:layout_marginTop="36dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:text="@string/settings_menu"
                        android:textColor="@color/super_white"
                        android:textSize="32sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_satellites_icon" />

                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>


            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/RC_Lanzamientos"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="16dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline_AprendeMas"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/guideline_LanzamientosHorizontal2" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_LanzamientosHorizontal2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="553dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_LanzamientosHorizontal1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="510dp" />

            <TextView
                android:id="@+id/TV_PreximosLanzamientos"
                android:layout_width="274dp"
                android:layout_height="0dp"
                android:fontFamily="@font/ralewaybold"
                android:gravity="bottom|start"
                android:padding="5dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/H_procLanzamientos"
                android:textColor="@color/white"
                android:textSize="22sp"
                app:layout_constraintBottom_toTopOf="@+id/guideline_LanzamientosHorizontal2"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/guideline_LanzamientosHorizontal1"
                app:layout_constraintVertical_bias="0.0" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Guideline_noticiasHorizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="177dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Guideline_Right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_end="16dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Guideline_Left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="16dp" />

            <TextView
                android:id="@+id/TV_VerMasNoticias"
                android:layout_width="106dp"
                android:layout_height="0dp"
                android:fontFamily="@font/ralewayregular"
                android:gravity="bottom|end"
                android:padding="5dp"
                android:paddingLeft="10dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/H_verMas"
                android:textColor="@color/white"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/Guideline_noticiasHorizontal"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/TV_UltimasNoricias"
                app:layout_constraintTop_toTopOf="@+id/guideline11" />

            <TextView
                android:id="@+id/TV_VerMasLanzamientos"
                android:layout_width="101dp"
                android:layout_height="43dp"
                android:layout_marginStart="20dp"
                android:fontFamily="@font/ralewayregular"
                android:gravity="bottom|end"
                android:padding="5dp"
                android:paddingLeft="10dp"
                android:paddingTop="20dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/H_verMas"
                android:textColor="@color/white"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/guideline_LanzamientosHorizontal2"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/TV_PreximosLanzamientos"
                app:layout_constraintTop_toTopOf="@+id/guideline_LanzamientosHorizontal1"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/TV_UltimasNoricias"
                android:layout_width="196dp"
                android:layout_height="49dp"
                android:fontFamily="@font/ralewaybold"
                android:gravity="bottom|start"
                android:padding="5dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/H_ultNoticias"
                android:textColor="@color/white"
                android:textSize="22sp"
                app:layout_constraintBottom_toTopOf="@+id/Guideline_noticiasHorizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/guideline11" />

            <TextView
                android:id="@+id/TV_HomeTitle"
                android:layout_width="194dp"
                android:layout_height="96dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/ralewaybold"
                android:gravity="center"
                android:padding="5dp"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="@string/app_name"
                android:textColor="@color/white"
                android:textSize="46sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/RC_Noticias"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="16dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline_LanzamientosHorizontal1"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/Guideline_noticiasHorizontal"
                app:layout_constraintVertical_bias="1.0" />

            <androidx.cardview.widget.CardView
                android:id="@+id/CV_Marte"
                style="@style/Base.CardView"
                android:layout_width="172dp"
                android:layout_height="219dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/guideline_AprendeMas2">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/base_cardview"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.511"
                    tools:layout_editor_absoluteX="-55dp">


                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/Guideline_abajo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_end="10dp" />

                    <ImageView
                        android:id="@+id/BG_Marte"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:background="@drawable/background_cardmarte"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/TV_Marte3d"
                        android:layout_width="142dp"
                        android:layout_height="39dp"
                        android:fontFamily="@font/ralewaybold"
                        android:gravity="start|left"
                        android:padding="5dp"
                        android:paddingLeft="10dp"
                        android:paddingTop="5dp"
                        android:paddingRight="5dp"
                        android:paddingBottom="5dp"
                        android:text="@string/H_marte3d"
                        android:textColor="@color/white"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.533"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.766" />

                    <TextView
                        android:id="@+id/TV_Pruebalo"
                        android:layout_width="143dp"
                        android:layout_height="0dp"
                        android:fontFamily="@font/ralewaylight"
                        android:gravity="start|top"
                        android:padding="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:paddingBottom="15dp"
                        android:text="@string/H_pruebalo"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_constraintBottom_toTopOf="@+id/Guideline_abajo"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.551"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/TV_Marte3d"
                        app:layout_constraintVertical_bias="0.0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/CV_Satelites"
                style="@style/Base.CardView"
                android:layout_width="172dp"
                android:layout_height="219dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintTop_toTopOf="@+id/guideline_AprendeMas2">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/base_cardview"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.511"
                    tools:layout_editor_absoluteX="-55dp">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/Guideline_abajo2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_end="10dp" />

                    <ImageView
                        android:id="@+id/BG_Satelites"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:background="@drawable/background_cardsatelites"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/TV_Satelites"
                        android:layout_width="146dp"
                        android:layout_height="68dp"
                        android:fontFamily="@font/ralewaybold"
                        android:gravity="start|left"
                        android:padding="5dp"
                        android:paddingLeft="10dp"
                        android:paddingTop="5dp"
                        android:paddingRight="5dp"
                        android:paddingBottom="5dp"
                        android:text="@string/H_satelites"
                        android:textColor="@color/white"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.615"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.754" />

                    <TextView
                        android:id="@+id/TV_Pruebalo2"
                        android:layout_width="145dp"
                        android:layout_height="0dp"
                        android:fontFamily="@font/ralewaylight"
                        android:gravity="start|top"
                        android:padding="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:paddingBottom="15dp"
                        android:text="@string/H_pruebalo"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_constraintBottom_toTopOf="@+id/Guideline_abajo2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.592"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/TV_Satelites"
                        app:layout_constraintVertical_bias="0.0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <ProgressBar
                android:id="@+id/progressBar_noticias"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="invisible"
                app:layout_constraintBottom_toTopOf="@+id/guideline_LanzamientosHorizontal1"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/Guideline_noticiasHorizontal" />

            <ProgressBar
                android:id="@+id/progressBar_lanzamientos"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="invisible"
                app:layout_constraintBottom_toTopOf="@+id/guideline_AprendeMas"
                app:layout_constraintEnd_toStartOf="@+id/Guideline_Right"
                app:layout_constraintStart_toStartOf="@+id/Guideline_Left"
                app:layout_constraintTop_toTopOf="@+id/RC_Lanzamientos" />

            <eightbitlab.com.blurview.BlurView
                android:id="@+id/blur_card"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:background="@drawable/blur_bg_lanzamiento"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <View
                    android:id="@+id/V_sesac"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </eightbitlab.com.blurview.BlurView>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="128dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>